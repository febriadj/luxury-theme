	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#dfd3fd3d"
			}
		},
		{
			"name": "Variables",
			"scope": ["variable", "string constant.other.placeholder"],
			"settings": {
				"foreground": "#eee1ffe3"
			}
		},
		{
			"name": "Colors",
